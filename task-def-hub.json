[
  {
    "dnsSearchDomains": null,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${cloudwatch_log_group_name}",
        "awslogs-region": "${aws_region}",
        "awslogs-stream-prefix": "${app_name}-${app_env}"
      }
    },
    "entryPoint": null,
    "portMappings": [
      {
        "hostPort": 0,
        "protocol": "tcp",
        "containerPort": 80
      }
    ],
    "command": null,
    "linuxParameters": null,
    "cpu": ${cpu},
    "environment": [
      {
        "name": "ADMIN_EMAIL",
        "value": "${admin_email}"
      },
      {
        "name": "ADMIN_NAME",
        "value": "${admin_name}"
      },
      {
        "name": "ADMIN_PASS",
        "value": "${admin_pass}"
      },
      {
        "name": "ANALYTICS_ID",
        "value": "${analytics_id}"
      },
      {
        "name": "BASE_URL_PATH",
        "value": "https://${subdomain}.${cloudflare_domain}/"
      },
      {
        "name": "DYNAMO_ACCESS_KEY_ID",
        "value": "${dynamo_access_key_id}"
      },
      {
        "name": "DYNAMO_SECRET_ACCESS_KEY",
        "value": "${dynamo_secret_access_key}"
      },
      {
        "name": "HELP_CENTER_URL",
        "value": "${help_center_url}"
      },
      {
        "name": "HUB_MODE",
        "value": "true"
      },
      {
        "name": "LOGGING_LEVEL",
        "value": "${logging_level}"
      },
      {
        "name": "MYSQL_HOST",
        "value": "${mysql_host}"
      },
      {
        "name": "MYSQL_DATABASE",
        "value": "${mysql_database}"
      },
      {
        "name": "MYSQL_USER",
        "value": "${mysql_user}"
      },
      {
        "name": "MYSQL_PASSWORD",
        "value": "${mysql_password}"
      },
      {
        "name": "SSL_CA_BASE64",
        "value": "${ssl_ca_base64}"
      },
      {
        "name": "SECRET_SALT",
        "value": "${secret_salt}"
      },
      {
        "name": "SESSION_STORE_TYPE",
        "value": "${session_store_type}"
      },
      {
        "name": "SHOW_SAML_ERRORS",
        "value": "${show_saml_errors}"
      },
      {
        "name": "ENABLE_DEBUG",
        "value": "${enable_debug}"
      },
      {
        "name": "THEME_COLOR_SCHEME",
        "value": "${theme_color_scheme}"
      }
    ],
    "ulimits": null,
    "dnsServers": null,
    "mountPoints": [],
    "workingDirectory": null,
    "secrets": null,
    "dockerSecurityOptions": null,
    "memory": ${memory},
    "memoryReservation": ${memory},
    "volumesFrom": [],
    "image": "${docker_image}:${docker_tag}",
    "disableNetworking": null,
    "interactive": null,
    "healthCheck": null,
    "essential": true,
    "links": null,
    "hostname": null,
    "extraHosts": null,
    "pseudoTerminal": null,
    "user": null,
    "readonlyRootFilesystem": null,
    "dockerLabels": null,
    "systemControls": null,
    "privileged": null,
    "name": "hub"
  }
]
